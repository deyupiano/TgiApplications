@model TgiApplications.Models.AllroleandUser
@{
    ViewBag.Title = "View Users Role";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


@using GridMvc.Html

<h2>Display All User Roles</h2>

<link href="~/Content/Gridmvc.css" rel="stylesheet" />

@*<link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-1.9.1.min.js"></script>*@

@Html.Grid(Model.AllDetailsUserlist).Columns(columns =>

{

columns.Add(c => c.UserName).Titled("UserName").Filterable(true);

columns.Add(c => c.RoleName).Titled("Name").Filterable(true);

}).WithPaging(10).Sortable(true)

@section scripts{
    <script src="~/Scripts/jquery-1.11.3.min.js"></script>
    <script src="~/Scripts/gridmvc.js"></script>
}

